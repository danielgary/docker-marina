{
    "server_name": "marina-test",
    "containers": [
        {
            "name": "nginx-test",
            "image": "hello-world-nginx",
            "tag": "latest",
            "environment": {
               
                "PORT": "3000"
               
            },
            "ports": [
                {
                    "public": "3000",
                    "private": "3000"
                }
            ],
            "web": {
                "server_names": [
                    "localhost"
                ],
                "location": "http://127.0.0.1:3000",
                "listen": 80
            }
        }
    ]
}